{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nestar/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nestar/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nestar",
  "projects": {
    "nestar": {
      "type": "application",
      "root": "apps/nestar",
      "entryFile": "main",
      "sourceRoot": "apps/nestar/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nestar/tsconfig.app.json"
      }
    },
    "nestar-batch": {
      "type": "application",
      "root": "apps/nestar-batch",
      "entryFile": "main",
      "sourceRoot": "apps/nestar-batch/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nestar-batch/tsconfig.app.json"
      }
    },
    "nestars-batch": {
      "type": "application",
      "root": "apps/nestars-batch",
      "entryFile": "main",
      "sourceRoot": "apps/nestars-batch/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nestars-batch/tsconfig.app.json"
      }
    }
  }
}